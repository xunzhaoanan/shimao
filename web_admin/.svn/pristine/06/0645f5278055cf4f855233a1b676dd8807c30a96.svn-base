.price_sell{color:#EFAA4D;}
.comFooter .info{ color:#555}
.goodsPrice .price_sell em{ color:#EFAA4D;}

/*商城*/
.list_font{ display: none;}
/**************模版样式*****************/
.home_block{ margin:0; width:auto; padding:5px;}
.home_block li{overflow: hidden; color: #fff;-webkit-box-sizing:border-box; -moz-box-sizing: border-box; /* box-sizing: border-box; */ height: 100px; width:33.3%!important; display: inline-block; float: left; padding: 2px;-webkit-background-clip:content; -moz-background-clip: content; background-clip: content; text-align: center; }
.home_block .box{color:#fff; line-height:normal; height:100%;}
.home_block li:nth-child(3n+1) .box{ background:#d87570;}
.home_block li:nth-child(3n+2) .box{ background:#bbd1c4}
.home_block li:nth-child(3n+3) .box{background:#febf02;}
.home_block li:nth-of-type(6n-2) .box{background:#bc4040;}
.home_block li:nth-of-type(6n-1) .box{background:#6f9c97;}
.home_block li:nth-of-type(6n) .box{background:#ff8b00;}
.home_block .figure{ margin:10px 0;}
.home_block .figure > i:before{ color:#fff; font-size:30px;}
.home_block .describe { font-size:1.6rem;}
