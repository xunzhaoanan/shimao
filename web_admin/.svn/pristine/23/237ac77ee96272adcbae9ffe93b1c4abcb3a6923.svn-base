.preview .only-view { width: 686px; margin: 0 auto; position: relative; left: 36px; }
.preview .message { float: left; width: 230px; background-color: #F2F2F2; margin-right: 52px; border-radius: 6px; -webkit-border-radius: 6px; -moz-border-radius: 6px; position: relative; top: 141px; }
.preview .message .error-msg { color: red; margin-top: 10px; margin-bottom: -10px; margin-left: 15px; margin-right: 15px; font-size: 14px; }
.preview .message .title-img { width: 20px; height: 10px; background: #8EC31F; border-radius: 6px; -webkit-border-radius: 6px; -moz-border-radius: 6px; position: absolute; top: -4px; left: 20px; }
.preview .message .message-row { float: left; width: 200px; margin-top: 20px; margin-left: 15px; }
.preview .message .message-row input[type='text'] { font-size: 16px; text-indent: 4px; width: 143px; height: 28px; border: 1px solid #ABADB3; }
.preview .message .message-row span { line-height: 28px; float: left; width: 52px; font-size: 17px; display: block; text-align: left; }
.preview .message .show { float: left; background-color: #E4E4E4; width: 200px; min-height: 84px; margin-left: 14px; margin-top: 20px; border: 1px solid #8EC31F; border-radius: 6px; margin-bottom: 3px; position: relative; overflow: hidden; padding-bottom: 7px; }
.preview .message .show form { position: absolute; height: 62px; width: 62px; left: 8px; top: 9px; background: url("/img/magazine/add-upload-bg.png"); cursor: pointer; margin-bottom: 10px; float: left; opacity: 0; }
.preview .message .show #file-btn { position: absolute; top: 0; left: 0; height: 62px; width: 62px; border-radius: 1px; border: 1px #e3e3e3 solid; padding-left: 10px; padding-right: 10px; opacity: 0; }
.preview .message .show button { font-size: 12pt; height: 27px; width: 27px; border: none; border-radius: 1px; color: white; background: url("/img/magazine/add-upload-img.png"); margin-left: 15px; margin-top: 15px; }
.preview .message .show .title { word-break: break-all; margin-top: 7px; margin-left: 12px; font-size: 14px; }
