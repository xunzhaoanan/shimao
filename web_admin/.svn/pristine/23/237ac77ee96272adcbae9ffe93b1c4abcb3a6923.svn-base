.preview .message .show .cover-wrapper img { width: 60px; height: 60px; }
.preview .message .show .cover-wrapper .upload-img { width: 62px; height: 62px; cursor: pointer; opacity: 0.7; position: absolute; left: -1px; top: -1px; display: none; }
.preview .message .show .cover-wrapper .upload-bg { width: 62px; height: 62px; }
.preview .message .show .cover-wrapper .add-bg { height: 27px; width: 27px; position: absolute; left: 17px; top: 17px; }
.preview .message .show .cover-wrapper #cover-img { border-radius: 4px; }
.preview .message .show .description { float: left; width: 111px; vertical-align: top; font-size: 13px; line-height: 13pt; word-break: break-all; margin-left: 20px; margin-top: 10px; }
.preview .message .upload-text { font-weight: 400; font-style: normal; font-size: 13px; color: #333333; text-align: left; margin-left: 17px; margin-bottom: 20px; }
.preview .iphone { float: left; position: relative; height: 579px; width: 314px; margin-left: 12px; margin-top: -12px; }
.preview .iphone iframe { border: none; width: 224px; height: 343px; top: 153px; left: 45px; background-color: white; z-index: 999; position: absolute; }
.preview .iphone img { top: 0px; position: absolute; width: 314px; height: 600px; float: left; }
