/*popover*/
.popover.bottom {
  margin-top: 5px;
}

.popover {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1010;
  display: none;
  padding: 5px;
  background-color: transparent;
  border: none;
  -webkit-box-shadow: none;
  box-shadow: none;
  max-width: none;
}

.popover.bottom .arrow {
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 10px;
  border-color: transparent transparent #c5c5c5 transparent;
  position: absolute;
  left: 50%;
  top: -15px;
  margin-left: -10px;
}

.popover.bottom .arrow:after {
  content: "";
  border-bottom-color: #FFFFFF;
  top: -9px;
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 10px;
  border-color: transparent transparent #fff transparent;
  position: absolute;
  left: 50%;
  margin-left: -10px;
}

.popover.right .arrow {
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 10px;
  border-color: transparent transparent transparent #c5c5c5;
  position: absolute;
  right: 0%;
  top: 28px;
  left: auto;
  margin-right: -15px;

}

.popover.right .arrow:after {
  content: "";
  border-bottom-color: #FFFFFF;
  top: -10px;
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 10px;
  border-color: transparent transparent transparent #fff;
  position: absolute;
  right: 0;
  left: auto;
  margin-right: -8px;
}

.popover .arrow:after {
  border-width: 10px;
  content: "";

}

.popover .arrow, .popover .arrow:after {
  position: absolute;
  display: block;
  width: 0;
  height: 0;
  border-color: transparent;
  border-style: solid;
}

.popover-inner {
  width: 312px;
  overflow: hidden;
  border-radius: 4px;
  -webkit-box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
  box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
}

.popover-content {
  padding: 10px;
  background-color: #ffffff;
  border-radius: 3px;
  background-clip: padding-box;
  border: 1px solid #c5c5c5;
}

.popover-link input.link-placeholder {
  font-size: 12px;
  width: 200px;
}

.colorpicker-element {
  cursor: default !important;
}

#color {
  width: 100%;
  height: 24px;
}

