.app-module .custom-image {
  width: 100%;
  overflow: hidden;
  display: none;
}

.app-module .custom-image ul {
  margin: 0;
  padding: 0;
}

.app-module .custom-image li {
  position: relative;
  min-height: 40px;
}

.app-module .custom-carousel .title, .custom-image .title {
  position: absolute;
  bottom: 0;
  left: 0;
