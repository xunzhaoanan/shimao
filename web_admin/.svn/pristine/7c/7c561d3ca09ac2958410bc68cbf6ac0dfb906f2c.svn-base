html, body, ol, ul, li, h1, h2, h3, h4, h5, h6, p, th, td, dl, dt, dd, form, fieldset, legend, input, textarea, select, div {
  margin: 0;
  padding: 0;
  word-break: break-all;
  word-wrap: break-word;
  white-space: normal;
  text-indent: 0;
  border: 0;
  vertical-align: baseline;
}

article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {
  display: block;
}

body {
  width: 100%;
  height: 100%;
  min-height: 100%;
  line-height: 1;
  background: #f0f0f0;
  -webkit-text-size-adjust: 100%;
  font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
}

ol, ul {
  list-style: none;
}

blockquote, q {
  quotes: none;
}

input, textarea {
  outline: 0;
  resize: none;
}

i, em {
  font-style: normal;
}

img {
  border: 0;
  vertical-align: middle;
  max-width: 100%;
  width: 100%;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}

label {
  cursor: pointer;
}

.clearfix:before, .clearfix:after {
  display: table;
  content: " "
}

.clearfix:after {
  clear: both
}

.shop-good .btn-primary.active {
  background-color: #d15b47 !important;
  border-color: #d15b47;
}

.lbl:before {
  margin-top: -5px;
}

.mall-page-manage {
  width: 850px;
  margin: auto;
  padding: 70px 0 125px;
  margin-bottom: 50px;
}

.app-container {
  width: 348px;
  border-radius: 50px;
  border: 1px solid #e5e5e5;
  float: left;
  position: relative;
}

.app-container .app-header {
  height: 70px;
  background: url('/ace/images/page/iphone_head.png') no-repeat center center
}

.app-container .app-body {
  position: relative;
  width: 320px;
  margin: 0 auto;
  padding-bottom: 11px;
  min-height: 568px;
  border: 1px solid #c5c5c5;
  background: #f0f0f0
}

.app-container .app-foot {
  position: relative;
  margin: 5px 0;
  height: 60px;
  background: url('/ace/images/page/iphone_foot.png') no-repeat center center
}

.app-container .app-field {
  margin: 0;
  cursor: pointer;
  padding: 18px 60px 0 60px;
  font-size: 16px;
  line-height: 46px;
  min-height: 64px;
  color: #FFFFFF;
  font-weight: bold;
  text-align: center;
  background: url("/ace/images/page/titlebar.png") no-repeat;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.app-setting-panel {
  position: relative;
  float: left;
  width: 480px;
  padding-bottom: 20px;
  font-size: 14px
}

.app-setting-panel > div {
  margin: 134px 0 20px 20px;
  position: relative
}

.app-setting-panel > div form {
  padding-top: 0px;
  background: #FFFFFF;
  border-radius: 5px;
  border: 1px solid #c5c5c5;
}

.app-setting-panel .delGoods {
  position: absolute;
  top: -8px;
  right: -8px;
  width: 18px;
  height: 18px;
  font-size: 14px;
  line-height: 18px;
  border-radius: 9px;
  color: #FFFFFF;
  text-align: center;
  cursor: pointer;
  background: rgba(153, 153, 153, 0.6)
}

.app-setting-panel .delGoods:hover {
  background-color: #3C8FD9;
  text-decoration: none
}

.app-setting-panel .arrow, .app-setting-panel .arrow:after {
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 8px 10px 8px 0;
  border-color: transparent #c5c5c5 transparent transparent;
  position: absolute;
  left: -10px;
  top: 14px
}

.app-setting-panel .arrow:after {
  content: "";
  border-right-color: #fff;
  left: 2px;
  top: -8px
}

.app-components {
  position: absolute;
  left: 0;
  bottom: auto;
  margin: 10px 0 80px;
  padding: 0 10px 4px 10px;
  border: 1px solid #ddd;
  background-color: #FFFFFF
}

.app-components .app-component {
  width: 20%;
  float: left;
  padding: 5px;
  text-align: center
}

.app-components .arrow, .app-components .arrow:after {
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 10px;
  border-color: transparent transparent #c5c5c5 transparent;
  position: absolute;
  left: 50%;
  top: -20px;
  margin-left: -10px
}

.app-components .arrow:after {
  content: "";
  border-bottom-color: #FFFFFF;
  top: -9px;
}

.app-components .app-component a {
  background-color: #fff;
  font-size: 12px;
  display: table-cell;
  width: 60px;
  height: 50px;
  padding: 10px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  border: 1px solid #d1d1d1;
  line-height: 14px;
  vertical-align: middle;
  cursor: pointer;
  text-decoration: none;
  color: #08c;
}

.app-components .app-component a:hover {
  color: #f00
}

.p-2 {
  padding: 2px !important
}

.p-4 {
  padding: 4px !important
}

.full-screen {
  width: 100%;
  height: auto;
}

.img-box {
  display: block;
  width: 100%;
  position: relative;
  float: none;
}

.navimg-box {
  display: block;
  width: 80%;
  margin: 0 auto;
  position: relative;
  float: none;
  vertical-align: top;
}

.img-box:after, .navimg-box:after {
  display: block;
  width: 100%;
  padding-top: 100%;
  content: '';
  position: relative;
  float: none;

}

.img-box-2:after {
  display: block;
  padding-top: 50%;
  content: '';
}

.img-box > img, .navimg-box img {
  width: 100%;
  height: 100%;
