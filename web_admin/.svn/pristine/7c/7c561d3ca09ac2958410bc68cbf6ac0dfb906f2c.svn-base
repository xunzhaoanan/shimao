/*图片广告编辑*/
.control-com .items {
  position: relative;
  border: 1px solid #e5e5e5;
  margin: 0 10px 15px 10px;
  padding: 15px;
  background: #f9f9f9;
  zoom: 1;
  clear: both;
}

.control-com .items:before, .control-com .items:after {
  display: table;
  content: " " clear : both
}

.control-com .items .img-box {
  width: 120px;
  position: relative;
  text-align: center;
  border: 1px solid #e5e5e5;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  overflow: hidden;
  float: left;
}

.control-com .items .img-box .thumb-image {
  min-height: 120px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  transition: all .5s;
}

.control-com .items .img-box .thumb-image:hover {
  transform: scale(1.2);
  -webkit-transform: scale(1.2);
  -moz-transform: scale(1.2);
  -ms-transform: scale(1.2);
}

.control-com .items .upload-img {
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  line-height: 30px;
  font-size: 14px;
  color: #fff;
  text-align: center;
  cursor: pointer;
  transition: all .3s;
  z-index: 100;
}

.control-com .items .upload-img:hover {
  background-color: rgba(0, 0, 0, 0.7);
}

.control-com .add-option {
