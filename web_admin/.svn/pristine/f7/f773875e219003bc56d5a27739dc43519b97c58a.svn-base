* {
	font-family: "microsoft yahei", "SimHei", Helvetica, sans-serif;
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	background: transparent;
	text-shadow: none;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	-webkit-overflow-scrolling: touch;
}
html, body {
	width: 100%;
	height: 100%;
	background-color: #000;
	min-width: 320px;
}
canvas{z-index:1000;}
@-webkit-keyframes tipmove {  0% {
	-webkit-transform:translate3d(0,0,0);
 opacity: 0;
}
 50% {
 -webkit-transform:translate3d(0,-5px,0);
 opacity: 1;
}
 100% {
 -webkit-transform:translate3d(0,-10px,0);
 opacity: 0;
}
}
.arrow{position: absolute; width: 24px; height: 18px; margin: 0 auto; left: 0; right: 0; bottom: 10px; background: url("http://7te85s.com2.z0.glb.qiniucdn.com/web-swipe-tip.png"); background-repeat: no-repeat; background-size: 100% 100%; -webkit-animation: tipmove 1.5s infinite ease-in-out; z-index: 300;}
div.page div.form, div.pages div.form { color: white; overflow: hidden; background: #772d90; padding: 5px 0 10px 0; border-width: 0px; border-style: solid; }
div.page div.form .component, div.pages div.form .component { position: relative; top: 0; left: 0; }
div.page div.form p, div.pages div.form p { font-size: 14px; height: 32px; line-height: 32px; display: block; overflow: hidden; padding: 0 5%; width: 90%; margin: 5px 0; }
div.page div.form p input, div.pages div.form p input { font-size: 14px; border: 0; height: 32px; line-height: 32px; width: 70%; float: right; color: #333; text-indent: 4px; background:#fff; }
div.page div.form a, div.pages div.form a {font-size: 14px;  background: #fff; border-radius: 3px; text-decoration: none; height: 32px; line-height: 32px; padding: 0 10px; text-indent: 0; text-align: center; float: right; margin: 3px 5% 3px 0; color: #772d90; }
div.page div.form a:active, div.pages div.form a:active { background: #ccc; }
/*page_rainy+page_erase+page_pointer */
@font-face { font-family: '方正黄草'; font-weight: normal; font-style: normal; }
@font-face { font-family: '毛笔行书'; font-weight: normal; font-style: normal; }
@font-face { font-family: '华康少女'; font-weight: normal; font-style: normal; }
@font-face { font-family: '微软雅黑'; }
div#loading-container { z-index: 1000001; position: absolute; top: 0; left: 0; right: 0; bottom: 0; background: #fff; }
.clearfix {  *zoom: 1;
}
.clearfix:before, .clearfix:after { display: table; content: ""; line-height: 0; }
.clearfix:after { clear: both; }
.hide { display: none; }
.invisible { visibility: hidden; }
div.music { position: absolute; left: 0; bottom: 0; z-index: 9999; width: 60px; height: 50px; overflow: hidden; }
#music_icon { width: 100%; height: 100%;overflow: hidden; }
.music_note{ width: 52px; height: 50px; background: url(http://imgcache.vikduo.com/static/7f87e3f6e111e55779c1a61583580747.png) 50% 50% no-repeat; background-size: 40%;}
div.music .icon-audio-on {  width: 50px; height: 50px; position: absolute; z-index: 20; left: 2px;top: 0;}
div.music .icon-audio-on i.playmusic { display: block; width: 100%; height: 100%; background: url(http://imgcache.vikduo.com/static/aba7bef02ce343cae08faeaa4af4d3be.png) 50% 50% no-repeat; background-size: 60%; }
.icon-audio-on i.playmusic { -webkit-animation: playmusic 1s linear infinite;}
@-webkit-keyframes playmusic {  0% {
 -webkit-transform:rotate(0);
}
 100% {
 -webkit-transform:rotate(360deg);
}
}
div.music .icon-audio-off { width: 100%; height: 100%; background: url(http://imgcache.vikduo.com/static/ed8a615bf4a394c46a568380eb8bb0fc.png) 50% 50% no-repeat; background-size: 60%; position: absolute; z-index: 10; left: 0;top: 0;}

div.music .off .icon-audio-off {background-image: url(http://imgcache.vikduo.com/static/f71d3da24aaf1f88ab38711ea5bc55f4.png);}
div.music .off .icon-audio-on{ display: none; }

div.music .music_bg2  .icon-audio-on i.playmusic { background-image: url(http://imgcache.vikduo.com/static/b0c6559b876b3a710bb3b9fee19eb402.png) ;}
div.music .music_bg2  .icon-audio-off { background-image: url(http://imgcache.vikduo.com/static/2fee0bbcb3bf62428efb89407a6fb655.png)}
div.music .music_bg2.off .icon-audio-off { background-image: url(http://imgcache.vikduo.com/static/83411551df6eea3a5c94e3a2a68beafd.png);}

div.music .btn1 { height: 100%; width: 100%; }
div.music .btn1 .music_open { display: block; height: 100%; width: 100%; }
div.music .btn1 .music_close { display: none; height: 100%; width: 100%; }
div.music img { float: left; width: 25px; height: 25px; margin-right: 6px; }

div.browse { position: absolute; right: 10px; bottom: 10px; z-index: 9999;  padding: 3px 8px; border-radius: 5px;  overflow: hidden;font-size: 12px;color: #fff; background: rgba(0,0,0,.5); }
div.browse em { font-style: normal; display: none;}
div.browse span { padding-left: 20px; background: url(http://imgcache.vikduo.com/static/0381c95371a601c06aefda45717fbdfd.png) no-repeat 0 100%;}

div.pages { position: absolute; width: 100%; height:100%; top: 0; left: 0; -webkit-transform: translate3d(0, 0, 0); background-color: #333333; overflow:hidden; }
section.page { background-color: white; position: absolute; background-size: 100% 100% !important; width:100%; height:100%; overflow:hidden; -webkit-backface-visibility:hidden; backface-visibility:hidden; }
section.page .viewCount { background-color: rgba(18, 16, 16, 0.6); width: 100%; text-align: center; position: absolute; left: 0; bottom: 0; display: block; z-index: 310; }
section.page .viewCount span { float: left; color: #B0B0B0; padding: 2px; font-size: small; border-radius: 2px; }
section.page .freeReg { width: 100%; text-align: center; position: absolute; left: 0; bottom: 0; display: block; z-index: 310; }
section.page .freeReg span { padding: 4px; color: white; font-size: small; border-radius: 2px; }
section.page .freeReg span strong { font-size: 16px; }
section.page .freeReg span b { font-weight: normal; color: #772d90; font-size: 16px; }
div.pages .component { position: absolute; }
div.pages article{line-height:28px;}
.btn{padding:0 21px 3px;}
div.mask { background: rgba(0, 0, 0, 0.8); position: absolute; opacity: 0; left: 0; top: 0; height: 100%; width: 100%; z-index: -1; overflow: hidden; font-weight: bold; -webkit-animation-fill-mode: both; color: #fff; }
div.mask.maskIn { -webkit-animation-name: maskIn; -webkit-animation-duration: 0.4s; -webkit-animation-timing-function: linear; -webkit-animation-iteration-count: 1; }
div.mask.maskOut { -webkit-animation-name: maskOut; -webkit-animation-duration: 0.4s; -webkit-animation-timing-function: linear; -webkit-animation-iteration-count: 1; }
div.ucbody { background: rgba(0, 0, 0, 0.9); height: 100%; z-index: 110; position: fixed; bottom: 0; -webkit-animation-fill-mode: both; }
div.more_container { width: 190px; padding: 0 5px 0 0; right: -250px; border-left: 1px solid #000000; -webkit-animation-fill-mode: both; }
div.more_container.moreIn { -webkit-animation-name: moreIn; -webkit-animation-duration: 0.4s; -webkit-animation-timing-function: linear; -webkit-animation-iteration-count: 1; }
div.more_container.moreOut { -webkit-animation-name: moreOut; -webkit-animation-duration: 0.4s; -webkit-animation-timing-function: linear; -webkit-animation-iteration-count: 1; }
@-webkit-keyframes maskIn {  0% {
 opacity: 0;
 z-index: -1;
}
 100% {
 opacity: 0.5;
 z-index: 105;
}
}
@-webkit-keyframes moreIn {  0% {
 right: -250px;
 opacity: 0.5;
}
 100% {
 right: 0;
 opacity: 1;
}
}
@-webkit-keyframes maskOut {  0% {
 opacity: 0.5;
 z-index: 105;
}
 100% {
 opacity: 0;
 z-index: -1;
}
}
@-webkit-keyframes moreOut {  0% {
 right: 0;
 opacity: 1;
}
 100% {
 right: -250px;
 opacity: 0.5;
}
}
.text_center { text-align: center; }
.fenxiang { position: absolute; width: 100%; height: 100%; overflow: hidden; background-position: center; background-size: cover; background-image: url(http://7te85s.com2.z0.glb.qiniucdn.com/rabbit-fx.png); background-color: rgba(0, 0, 0, 0.9); z-index: -1; opacity: 0; }
@-webkit-keyframes loadAni {  from {
 opacity: 1;
 display: block;
}
to { opacity: 0; display: none; }
}
.loading { margin: -25px 0 0 -25px; position: absolute; left: 50%; top: 50%; width: 50px; height: 50px; }
.loading.fadeOut { -webkit-transition: opacity 0.4s; opacity: 0; }
.printcss { -webkit-animation: printcss 2s ease forwards; }
 @-webkit-keyframes printcss {  0% {
height:0;
}
 100% {
height: 100%;
}
}
.childPages{position:absolute; width:10px; right:20px; top:0; z-index:10000;}
.childPage{position:relative; width:10px; height:10px; margin:2px 0;  -webkit-box-sizing:border-box; border:1px solid #fff; border-radius:100%; background:transparent; opacity:0.5;}
.now{background:#fff; border:0; opacity:1;}

