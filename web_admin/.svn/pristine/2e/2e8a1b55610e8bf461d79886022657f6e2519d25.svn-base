@charset "utf-8";
/* CSS Document */

body, ol, ul, li, h1, h2, h3, h4, h5, h6, p, th, td, dl, dt, dd, form, fieldset, legend, input, textarea, select { margin: 0; padding: 0; word-break: break-all; word-wrap: break-word; white-space: normal; font-family: "microsoft yahei"; font-size: 12px; }
body { background: #fff; }
table { border-collapse: collapse; border-spacing: 0; }
ul, li { list-style: none; }
i, em { font-style: normal; }
a { text-decoration: none; }
img{max-width: 100%; border: 0;}
.clearfix, .clear, .content { clear: both; zoom: 1; }
.clearfix:before, .clear:before, .content:before { clear: both; content: ""; display: block; font-size: 0; height: 0; visibility: hidden; zoom: 1; }
.clearfix:after, .clear:after, .content:after { clear: both; content: ""; display: block; font-size: 0; height: 0; visibility: hidden; zoom: 1; }
.ico-font { display: inline-block; font: normal normal normal 14px/1 icomoon; font-size: inherit; text-rendering: auto; -webkit-font-smoothing: antialiased; }
.fl { float: left; }
.fr { float: right; }
/*顶部*/
.navigator { height: 45px; background-color: #3e94e1; }
.navigator .nav-left span.active, .navigator .nav-left .navigator-item:hover ,.navigator .nav-right span.active,.navigator .nav-right .navigator-item:hover { background-color: #ff6900;}
.magazine_logo { height: 45px; line-height: 40px; margin-right: 20px; display: inline-block; float: left; }
.navigator span.nav-left span.navigator-item, .navigator span.nav-right span.navigator-item, .navigator span.nav-left a.navigator-item, .navigator span.nav-right a.navigator-item { cursor: pointer; float: left; position: relative; vertical-align: top; height: 23px;margin-left: 0px; margin-right: 0px; border-right: 1px solid #3487d0; color: #fff; }
.navigator span.nav-right { display: inline-block; position: absolute; right: 0px; height: 45px; margin: 0; padding: 0px; line-height: 45px; }
.navigator span.nav-right a.navigator-item { padding: 0px 15px; display: inline-block; height: 45px; line-height: 45px; }
.navigator span.nav-center input.weza-name { padding: 0px 15px; color: #ffffff; height: 45px; background: transparent; border: 0; direction: ltr; width: auto; max-width: 572.96875px; text-align: center; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }
.fa-left { display: block; text-align: center; font: normal normal normal 14px/1 icomoon; font-size: 20px; text-rendering: auto; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale; }
.navigator span.nav-left img { vertical-align: middle; float: none; margin-top: 0px; margin-right: 0px; }
.navigator span.nav-left div.app-lists ul { background: none; color: #666;overflow: hidden; list-style-type: none; width: 100%;  margin: 0px; max-height: none; }
/* 我的应用列表 */
.app-lists{ position: absolute; top: 45px; left: 0px; border:1px solid #e9e8e8; background-color:rgba(255,255,255,.95); }
#app-history{width: 220px;}
#app-list{width: 400px;}
.navigator span.nav-left div#app-list ul li p{ width: 158px;}
.navigator span.nav-left div#app-history ul li p{width:111px;}


