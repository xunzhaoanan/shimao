/*商城*/
.list_font{ display: none;}
/*官网*/
.home_block{
    margin: 10px;
    overflow: hidden;
    padding: 5px;
    width:150px;
    margin:auto;
    margin-left:50px;
    padding:80px 0 50px 0;
}
.home_block .box{
    color:#666;
    display:block;
    background:#fff;
    line-height:35px;
    overflow:hidden;
}
.home_block li{
	float:none;
	width:auto;
    padding:2px;
    display:block;
    text-align:center;
    -webkit-box-sizing:border-box;
}
.home_block .pic{ display:none;}
.home_block .figure{ display:none;}

